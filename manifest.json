{
  "manifest_version": 3,
  "name": "Coffee Notes",
  "version": "1.0",
  "description": "A cozy, coffee-themed handwritten checklist for your daily tasks.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}